{
    "name": "vundi/php-evangelists",
    "description": "A package that tells you what type of evangelist you are depending on the number of public repos you have on github",
    "keywords": ["php evangelists"],
    "homepage": "https://github.com/andela-cvundi/checkpoint1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Christopher Vundi",
            "email": "chris.vundi@andela.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Vundi\\Checkpoint1\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vundi\\Checkpoint1\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1",
        "satooshi/php-coveralls": "^0.7.0"
    }
}
